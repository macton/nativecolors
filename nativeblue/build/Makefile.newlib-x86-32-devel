PROJECT  := libnativeblack-newlib-x86-32-devel.a
OBJS     := $(patsubst %.c, %.newlib-x86-32-devel.o, $(SOURCES))
TC_PATH  := $(abspath $(NACL_SDK_ROOT)/toolchain/$(OSNAME)_x86_newlib)
CFLAGS   += -DDEVEL -O2

include build/Makefile.common

%.newlib-x86-32-devel.o : %.c $(DEPS)
	$(CC) $(CFLAGS) -c $< -o $@
	$(STRIP) $@

